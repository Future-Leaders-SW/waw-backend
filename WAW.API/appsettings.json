{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:u} {Level}]: {Message:l}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "AllowedOrigins": [
    "*.wawupc.ga",
    "*.wawupc.tech",
    "*.waw-frontend.pages.dev"
  ]
}
